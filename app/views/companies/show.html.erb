<main>
  <div class="modal-body row">
    <div class="col-md-6">
      <h1>Jobs for <%= @company.name %></h1>
      <% @company.jobs.each do |job| %>
      <ul><%= job.title %></ul>
      <% end %>
    </div>
    <div class="col-md-6">
      <h1>Contacts</h1>
      <%= render partial:"contactform" %>
    </div>
  </div>
</main>
